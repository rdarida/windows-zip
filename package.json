  "license": "MIT",
  "keywords": [
    "nodejs",
    "javascript",
    "windows",
    "zip",
    "unzip"
  ],
  "funding": [
    {
      "type": "patreon",
      "url": "https://www.patreon.com/rdarida"
    }
  ],
  "homepage": "https://rdarida.github.io/windows-zip/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rdarida/windows-zip.git"
  },
  "bugs": {
    "url": "https://github.com/rdarida/windows-zip/issues"
  },
  "main": "dist/index.js",
  "types": "index.d.ts",
  "files": [
    "dist",
    "index.d.ts"
  ],
  "scripts": {
    "postinstall": "npx husky install",
    "format": "prettier --write \"**/{src,test}/*.{js,json}\"",
    "lint": "eslint ./{src,test}/**/*.js",
    "pretest": "rimraf coverage",
    "test": "jest",
    "predocs": "rimraf docs",
    "docs": "jsdoc -c jsdoc.config.json",
    "prebuild": "rimraf dist",
    "build": "babel src --out-dir dist",
    "release": "standard-version",
    "postrelease": "git push --follow-tags origin main",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/cli": "^7.13.10",
    "@babel/core": "^7.13.10",
    "@babel/preset-env": "^7.13.10",
    "@commitlint/cli": "^12.0.1",
    "@commitlint/config-conventional": "^12.0.1",
    "eslint": "^7.22.0",
    "husky": "^4.3.0",
    "jest": "^26.6.3",
    "jsdoc": "^3.6.6",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "standard-version": "^9.1.1"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "post-commit": "npm run format"
    }
  }
}
